<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="dashboardPane" prefHeight="360.0" prefWidth="580.0" style="-fx-background-color: #d4ddff#d4ddff;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.views.Dashboard">
   <children>
      <Button fx:id="addMedicine" layoutX="161.0" layoutY="79.0" mnemonicParsing="false" onAction="#addmedicinelistner" prefHeight="35.0" prefWidth="117.0" style="-fx-background-color: #88fc03#88fc03; -fx-border-style: solid; -fx-border-radius: 5px;" text="Add Medicine" />
      <Button fx:id="deleteMedicine" layoutX="228.0" layoutY="136.0" mnemonicParsing="false" onAction="#deleteMedicineListner" prefHeight="35.0" prefWidth="117.0" style="-fx-background-color: #fc3d03#fc3d03; -fx-border-style: solid; -fx-border-radius: 5px;" text="Delete Medicine" textFill="#fffdfd" />
      <Button fx:id="updateMedicine" layoutX="288.0" layoutY="185.0" mnemonicParsing="false" onAction="#updateMedicineListner" prefHeight="35.0" prefWidth="117.0" style="-fx-background-color: #f4fc03; -fx-border-style: solid; -fx-border-radius: 5px;" text="Update Medicine" />
      <Button fx:id="showAllMedicine" layoutX="350.0" layoutY="237.0" mnemonicParsing="false" onAction="#showAllMedicineListner" prefHeight="35.0" prefWidth="129.0" style="-fx-background-color: #035efc#035efc; -fx-border-style: solid; -fx-border-radius: 5px;" text="Show All Medicine" textFill="WHITE" />
      <Label fx:id="heading" layoutX="-80.0" layoutY="128.0" prefHeight="83.0" prefWidth="307.0" style="-fx-rotate: -90;" text="Medicine Store" textAlignment="CENTER">
         <font>
            <Font name="American Typewriter" size="40.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
